# YeevuDNS API
YEEVU_DNS_API_URL=https://yeevu-dns.domains-12a.workers.dev
BOOTSTRAP_SECRET=replace-with-bootstrap-secret

# Build-time
NEXT_PUBLIC_BASE_PATH=/dns
